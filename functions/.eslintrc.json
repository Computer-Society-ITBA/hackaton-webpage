{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "node": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "warn",
    "eqeqeq": ["error", "smart"],
    "no-unused-vars": "warn",
    "no-lonely-if": "warn",
    "no-else-return": "warn",
    "no-implicit-coercion": "warn",
    "no-eq-null": "warn",
    "no-eval": "warn",
    "no-extend-native": "warn",
    "camelcase": "warn",
    "no-undef": "warn",
    "no-useless-escape": "warn",
    "no-dupe-keys": "warn"
  }
}
